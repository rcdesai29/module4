<%- include('../partials/header.ejs')%>
<div class="content">
  <h4><%=story.title%></h4>
  <small>Posted <%=story.createdAt%> by <%=story.author%></small>
  <div id="story"><%=story.content%></div>
</div>
<form>
  <button
    type="submit"
    formmethod="GET"
    formaction="/stories/<%=story.id%>/edit"
  >
    Edit
  </button>
  <button
    type="submit"
    formmethod="post"
    formaction="/stories/<%=story.id%>?_method=DELETE"
  >
    Delete
  </button>
</form>
<%- include('../partials/footer.ejs')%>
